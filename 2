#include<iostream>
using namespace std;
int a[100001];//数组下标值都为0，0代表质数 
int b[101];//b数组用来存放输入的n个数 
int main(){
	bool as=true;//状态变量 
	int n;
	cout<<"请输入您要判断的数字的数量：";
	cin>>n;//输入数量 
	cout<<endl;
	for(int i=2;i<100000;i++){
		if(a[i]==0){
			for(int j=i*2;j<100000;j+=i){
				a[j]=1;//下标值为1代表合数 
			}
		}
	}
	cout<<"请输入您要判断的数字：";
	for(int i=0;i<n;i++){
		cin>>b[i];//输入要判断的数字  
		if(i==0){
			cout<<endl; 
			cout<<"质数是：";
		} 
		if(a[b[i]]==0){
			cout<<b[i]<<" ";//输出质数 
			as=false;//有质数输出，状态变量进行改变 
		}
	}
	if(as){
		cout<<"没有质数！";//如果状态变量改变就输出没有质数 
	}
    return 0;
}
